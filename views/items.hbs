{{#if prevPage}}
  <a href="/items?p={{prevPage}}">&lt;</a>
{{/if}}
<form>
  <input name="name" value="{{name}}"></input> 
  <button type="submit">search</button>
</form>
{{#if nextPage}}
  <a href="/items?p={{nextPage}}" class="float-right">></a>
{{/if}}
<table class="table table-striped table-sm">
  <thead class="thead-dark">
    <tr>
      <th scope="col">Name</th>
      <th scope="col">EQ ID</th>
      <th scope="col">MR ID</th>
    </tr>
  </thead>
  {{#each items}}
  <tr>
    <td><a href="/items/{{this.id}}">{{this.name}}</a></td>
    <td><a href="/items/{{this.id}}">{{this.itemId}}</a></td>
    <td><a href="/items/{{this.id}}">{{this.id}}</a></td>
  </tr>
  {{/each}}
</table>
{{#if prevPage}}
  <a href="/items?p={{prevPage}}">&lt;</a>
{{/if}}
{{#if nextPage}}
  <a href="/items?p={{nextPage}}" class="float-right">></a>
{{/if}}